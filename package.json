{
  "name": "pickup-hockey",
  "author": "David Kavanaugh",
  "version": "1.0.0",
  "description": "MERN stack pickup hockey app",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "proxy": "http://localhost:5000",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidkavanaugh/pickup-hockey.git"
  },
  "keywords": [
    "pickup hockey",
    "ice hockey",
    "hockey",
    "ice time",
    "rink management"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidkavanaugh/pickup-hockey/issues"
  },
  "homepage": "https://github.com/davidkavanaugh/pickup-hockey#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "config": "^3.2.2",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongo": "^0.1.0",
    "mongoose": "^5.6.7",
    "multer": "^1.4.2",
    "nodemon": "^1.19.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validation": "0.0.1",
    "validator": "^11.0.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
}
